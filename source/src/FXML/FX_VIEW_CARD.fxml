<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="FXID_MAIN_VIEW" prefHeight="400.0" prefWidth="600.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.views.UI_CardView">
   <top>
      <HBox fx:id="FXID_VIEW_TITLE" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" />
         </BorderPane.margin>
         <children>
            <Text fx:id="FXID_VIEW_TITLE_TEXT" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
         </children>
      </HBox>
   </top>
   <center>
      <StackPane BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="FXID_TABLE" maxHeight="10000.0" maxWidth="10000.0" prefHeight="359.0" prefWidth="611.0">
              <columns>
                  <TableColumn fx:id="FXID_COLUMN_NUMBER" maxWidth="1.7976931348623157E308" minWidth="61.0" prefWidth="116.0" styleClass="centercol" text="[ COLUMN_NUMBER ]" />
                <TableColumn fx:id="FXID_COLUMN_ID" maxWidth="1.7976931348623157E308" minWidth="60.0" prefWidth="79.0" styleClass="centercol" text="[ COLUMN_ID ]" />
                <TableColumn fx:id="FXID_COLUMN_NAME" maxWidth="1.7976931348623157E308" minWidth="75.0" prefWidth="135.0" styleClass="leftcol" text="[ COLUMN_NAME ]" />
                  <TableColumn fx:id="FXID_COLUMN_SURNAME" maxWidth="1.7976931348623157E308" minWidth="75.0" prefWidth="145.0" styleClass="centercol" text="[ COLUMN_SURNAME ]" />
                  <TableColumn fx:id="FXID_COLUMN_OPTIONS" maxWidth="1.7976931348623157E308" minWidth="75.0" prefWidth="175.0" styleClass="centercol" text="[ COLUM_OPTIONS ]" />
              </columns>
            </TableView>
         </children>
      </StackPane>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="49.0" prefWidth="600.0" styleClass="menubar" BorderPane.alignment="CENTER">
         <children>
            <Text fx:id="FXID_SEARCH_LABEL" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="regLabel" text="[ LABEL_SEARCH ]">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </Text>
            <TextField fx:id="FXID_SEARCH_FIELD" />
            <Button fx:id="FXID_SEARCH_BTN" mnemonicParsing="false" onAction="#btnSearchOnClick" prefHeight="25.0" prefWidth="93.0" styleClass="regButton" text="[ SEARCH ]">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Button>
            <Region prefHeight="43.0" prefWidth="105.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="FXID_ADD_BTN" alignment="CENTER" mnemonicParsing="false" onAction="#btnAddOnClick" prefHeight="29.0" prefWidth="158.0" styleClass="regButton" text="[ ADD ]">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
