              </columns>
            </TableView>
         </children>
      </StackPane>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="49.0" prefWidth="600.0" styleClass="menubar" BorderPane.alignment="CENTER">
         <children>
            <Text fx:id="FXID_LABEL_SEARCH" strokeType="OUTSIDE" strokeWidth="0.0" text="[ LABEL_SEARCH ]">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </Text>
            <TextField />
         </children>
      </HBox>
   </bottom>
</BorderPane>
