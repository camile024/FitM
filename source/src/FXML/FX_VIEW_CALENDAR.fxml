                           </children>
                        </StackPane>
                        <StackPane prefWidth="100.0" HBox.hgrow="ALWAYS">
                           <children>
                              <Text fx:id="FXID_D_SUN" boundsType="VISUAL" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="calendarLabel" text="[ SUN]" StackPane.alignment="CENTER" />
                           </children>
                        </StackPane>
                     </children>
                     <opaqueInsets>
                        <Insets />
