                                 </HBox.margin>
                              </Button>
                              <Button fx:id="FXID_NEXT_BTN" mnemonicParsing="false" onAction="#btnNextOnClick" prefHeight="27.0" prefWidth="117.0" styleClass="regButton" text="&gt;&gt;&gt;" />
